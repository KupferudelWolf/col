{
  "name": "Color Schemer",
  "version": "0.4.3",
  "timestamp": "December 2017",
  "description": "Displays input hexadecimal colors.",
  "main": "col.js",
  "author": "Avoyt",
  "dependencies": {
    "color-blocks": "^1.0.0",
    "discord.io": "^2.5.3",
    "discord.js": "^11.2.1",
    "discord.js-commando": "^0.9.0",
    "jimp": "^0.2.28",
    "node-png": "^0.4.3",
    "winston": "^2.4.0"
  },
  "update_notes": {
    "0.3": [
      "DECEMBER 2017"
      , "Added Color Gradient feature. (!col -g)"
      , "Improved help documentation."
      , "Added command-specific help documentation. (!col <command> -h)"
      , "Bot now allows for uppercase commands (i.e. !COL HELP)."
      , "Bot now allows for color to be input as 0xFFFFFF."
      , "Added many more preset colors."
      , "Added limit to input colors."
      , "Added update notes feature in -v."
    ],
    "0.4": [
      "DECEMBER 2017"
      , "Added Color Lookup feature. (!col -a)"
      , "Added CMYK, CIE-L*ab, and Y'UV conversion."
      , "Bot now allows for color to be input as a HSV value."
      , "Improved random color generation."
    ]
  },
  "devDependencies": {},
  "scripts": {
    "test": "!col #FF8345"
  },
  "keywords": [
    "color"
  ],
  "license": "ISC"
}
